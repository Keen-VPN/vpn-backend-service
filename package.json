{
  "name": "keenvpn-backend",
  "version": "2.0.0",
  "description": "KeenVPN Backend API with TypeScript, Prisma, and Stripe subscription management",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc && prisma generate",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "migrate:data": "tsx scripts/migrate-data.ts",
    "verify:migration": "tsx scripts/verify-migration.ts",
    "db:export": "tsx export-data-from-supabase.ts",
    "db:import": "tsx import-data-to-new-db.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^11.11.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "serverless-http": "^3.2.0",
    "stripe": "^14.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.9",
    "nodemon": "^3.0.2",
    "prisma": "^6.17.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "express",
    "api",
    "prisma",
    "supabase",
    "postgresql",
    "firebase",
    "stripe",
    "subscription",
    "netlify",
    "serverless"
  ],
  "author": "KeenVPN Team",
  "license": "MIT"
}
